class Solution(object):
    def reverse(self, x):
        """
        :type x: int
        :rtype: int
        """
        if x >= 0 :
            s = ""
        else:
            s = "-"
            x*=-1
        x = str(x)
        for i in range(len(x)-1,-1,-1):
            s += x[i]
        return int(s)
            
            
